<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Phone Number</th>
			<th>Gender</th>
		</tr>
	</thead>
	<tbody>
		{% for row in rows %}
			<tr>
				<td>{{ row.id }}</td>
				<td>{{ row.first_name }}</td>
				<td>{{ row.last_name }}</td>
				<td>{{ row.email }}</td>
				<td>{{ row.phone_number }}</td>
				<td>{{ row.gender }}</td>
			</tr>
		{% endfor %}
	</tbody>
</table>
